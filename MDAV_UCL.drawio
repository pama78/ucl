        <mxCell id="dGhhYwrYM2k6FMBEav-u-108" value="&lt;p class=&quot;MsoNoSpacing&quot;&gt;&lt;/p&gt;&lt;font face=&quot;calibri light, sans-serif&quot;&gt;&lt;span lang=&quot;CS&quot; style=&quot;background-color: rgb(255 , 255 , 0)&quot;&gt;&lt;p class=&quot;MsoNoSpacing&quot;&gt;&lt;b&gt;#dalsi atributy matic&amp;nbsp;&lt;br&gt;&lt;/b&gt;&lt;b&gt;&amp;nbsp;&lt;/b&gt; colnames(x)&amp;lt;-c(&quot;A&quot;,&quot;B&quot;, &quot;C&quot;)&amp;nbsp; &lt;br&gt;&amp;nbsp; rownames(x)&amp;lt;-c(&quot;R1&quot;,&quot;R2&quot;, &quot;R3&quot;)&amp;nbsp;&lt;br&gt;&amp;nbsp; rownames(x)&amp;lt;-paste0(&quot;R&quot;,1:3)&lt;br&gt;&amp;nbsp; attributes(x) #dimnames[[1]] [1] &quot;R1&quot; &quot;R2&quot; &quot;R3&quot; $dimnames[[2]]&amp;nbsp; #[1] &quot;A&quot; &quot;B&quot; &quot;C&quot;&amp;nbsp; prvni dimenze - radky, druha sloupce&lt;/p&gt;&lt;/span&gt;&lt;/font&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;" parent="1" vertex="1">
