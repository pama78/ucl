        </mxCell>
        <mxCell id="dGhhYwrYM2k6FMBEav-u-45" value="&lt;div&gt;&lt;b&gt;#1. apply - opakovani funkci pres dimenzi - data, margin=nta hrana a fun ...&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp;#colSums a rowSums - je pro soucet pres radky a sloupce&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp;#MARGIN= 1 - je realizace pres jednotlivé rádky, 2 - pro jednotlivé sloupce, 3 - by pak bylo pres tretí dimenzi vícedimenzionálního vektoru.&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; nase_matice&amp;lt;-matrix(1:9,3) ; nase_matice# Zadefinujeme si matici::&lt;/div&gt;&lt;div&gt;&amp;nbsp; rowSums(nase_matice) ; colSums(nase_matice) # Vypocet pouzitim funkci colSums a rowSums&lt;/div&gt;&lt;div&gt;&amp;nbsp; apply(nase_matice, 1, sum); apply(nase_matice, 2, sum) # Apply funkce prvni vpocita soucty po radcich a druha po sloupcich&lt;/div&gt;&lt;div&gt;&amp;nbsp; apply(nase_matice, 1, var); apply(nase_matice, 2, var) # rozptyl u kazdeho&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; nase_matice[2,2]&amp;lt;-NA ;print(nase_matice) # pridame do nasi matice NA hodnotu:&lt;/div&gt;&lt;div&gt;&amp;nbsp; apply(nase_matice, 1, sum); apply(nase_matice, 2, sum) ##vyzkousime prostou sumu bez argumentu na odstraneni NA hodnot:&amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; apply(nase_matice, 1, sum, na.rm=TRUE) ; apply(nase_matice, 2, sum, na.rm=TRUE) # S spravne nastavenym argumentem (ignoruje NA hodnotu pro vypocet)&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;" parent="1" vertex="1">
